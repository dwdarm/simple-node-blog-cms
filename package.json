    "crypto-random-string": "^3.2.0",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.4.16",
    "sequelize": "^6.3.0",
    "slugify": "^1.4.4",
    "sqlite3": "^5.0.0",
    "uuid": "^8.2.0",
