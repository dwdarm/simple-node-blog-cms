{
  "name": "simple-node-blog-cms",
  "version": "1.0.0",
  "description": "NodeJS headless blog CMS",
  "main": "index.js",
  "scripts": {
    "dev": "export NODE_ENV=development && nodemon index.js",
    "dev:client": "webpack-dev-server --config ./admin-client/webpack.dev.js",
    "build:client": "webpack --config ./admin-client/webpack.prod.js",
    "build": "npm run build:client",
    "start": "export NODE_ENV=production && node index.js ",
    "test:unit": "export NODE_ENV=test && mocha --recursive ./internal/**/*.spec.js ./app/**/*.spec.js",
    "test:integration": "export DB_LOG=false && export NODE_ENV=test && mocha --recursive ./tests"
  },
